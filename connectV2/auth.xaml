<Window x:Class="connectV2.auth"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:connectV2"
        mc:Ignorable="d"
        Title="auth" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="login" HorizontalAlignment="Left" Height="23" Margin="76,88,0,0" TextWrapping="Wrap" Text="1" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="pass" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="1" VerticalAlignment="Top" Width="120" Margin="76,52,0,0"/>
        <Button Content="Авторизация" HorizontalAlignment="Left" Margin="76,125,0,0" VerticalAlignment="Top" Width="119" Click="Button_Click" Height="31"/>

    </Grid>
</Window>
